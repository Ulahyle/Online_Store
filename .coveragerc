[run]
source = .
omit =
    */migrations/*
    */tests.py
    config/*
    manage.py
    core/admin.py
    core/apps.py
    core/views.py
    accounts/admin.py
    accounts/apps.py
    cart/admin.py
    cart/apps.py
    market/admin.py
    market/apps.py
    order/admin.py
    order/apps.py
    reviews/admin.py
    reviews/apps.py

[report]
show_missing = True
skip_covered = True
